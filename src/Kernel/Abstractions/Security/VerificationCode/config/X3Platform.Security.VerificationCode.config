<configuration>
  <!-- verification code configuration begin -->
  <security.verificationCode>
    <keys>
      <!-- SQL Server : 数据库连接名称 -->
      <add name="DataSourceName" value="ConnectionString" />
      <!-- IBatis : 配置映射文件 -->
      <add name="IBatisMapping" value="config\X3Platform.Security.VerificationCode.IBatis.SqlMap.config" />
    </keys>
  </security.verificationCode>
  <!-- verification code configuration end -->
</configuration>