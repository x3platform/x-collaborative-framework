<?xml version="1.0" encoding="utf-8" ?>
<!--
    /*
     * The Contact application block configuration file.
     *
     * Update : 2010.01.01
     */     
     
     DataSourceName : ConnectionString (SQLServer) | config\X3Platform.Membership.IBatis.SqlMap.config (IBatis) 
-->
<configuration>
    <plugins.contacts>
        <keys>
            <!-- SQL Server : 数据库连接名称 -->
            <add name="DataSourceName" value="ConnectionString" />
            <!-- Spring.NET : 配置文件 -->
            <add name="SpringObjectFile" value="config/X3Platform.Plugins.Contacts.Spring.config" />
            <!-- IBatis : 配置映射文件 -->
            <add name="IBatisMapping" value="config/X3Platform.Plugins.Contacts.IBatis.SqlMap.config" />
        </keys>
    </plugins.contacts>
</configuration>