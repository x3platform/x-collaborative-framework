@{ Layout = "~/views/templates/themes/" + ViewBag.ThemeName + "/render.cshtml"; }
<script type="text/javascript" id="crypto-sha1-script" src="/resources/scripts/crypto/rollups/sha1.js"></script>
<script type="text/javascript" src="/resources/scripts/app/x.app.js"></script>
<script type="text/javascript" src="/views/shared/wizards/contacts-wizard.js"></script>
<script type="text/javascript" src="/views/main/membership/account-list.js"></script>
@Html.Raw(X3Platform.Web.Builder.BuilderContext.Instance.MenuManagement.GetMenu(ViewBag.account, ViewBag.application.ApplicationName, new Dictionary<string, string> { { "currentMenuFullPath", @"人员及权限管理\帐号管理" } }))
<div id="window-container" class="window-container">
  <div id="window-main-table" class="table">
    <div id="window-main-table-header" class="table-header x-freeze-height">
      <span>帐号管理</span>
    </div>
    <div id="window-main-table-body" class="table-body">
        <input id="defaultPassword" name="defaultPassword" type="hidden" value="888888" />
      <table class="table table-nopadding">
        <tr>
          <td class="table-sidebar">
            <div id="window-main-table-sidebar">
              <div class="table-sidebar-search form-inline">
                <input id="searchText" type="text" value="" class="table-sidebar-search-text form-control input-sm" placeholder="帐号\姓名" /> 
                <button id="btnFilter" class="btn btn-default btn-sm" title="查询"><i class="glyphicon glyphicon-search"></i></button>
              </div>
              <div id="treeViewContainer" class="table-sidebar-tree-view"></div>
            </div>
          </td>
          <td>
            <div id="window-main-table-container"><span class="tooltip-loading-text "><img src="/resources/images/loading.gif" alt="正在加载..." /></span></div>
          </td>
        </tr>
      </table>
    </div>
    <div id="window-main-table-footer" class="table-footer x-freeze-height"></div>
  </div>
</div>