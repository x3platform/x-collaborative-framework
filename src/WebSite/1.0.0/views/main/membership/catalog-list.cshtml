@{ Layout = "~/views/templates/themes/" + ViewBag.ThemeName + "/render.cshtml"; }
<script type="text/javascript" id="x-app-script" src="/resources/scripts/app/x.app.js"></script>
<script type="text/javascript" id="main-membership-catalog-list-script" src="/views/main/membership/catalog-list.js"></script>
@Html.Raw(X3Platform.Web.Builder.BuilderContext.Instance.MenuManagement.GetMenu(ViewBag.account, ViewBag.application.ApplicationName, new Dictionary<string, string> { { "currentMenuFullPath", @"人员及权限管理\分组类别设置" } }))
<div id="window-container" class="window-container">
  <div id="window-main-table" class="table">
    <div id="window-main-table-header" class="table-header x-freeze-height">
      <span>目录设置</span>
    </div>
    <div id="window-main-table-body" class="table-body">
      <div class="table-row-filter form-inline text-right x-freeze-height">
        <input id="searchText" type="text" value="" class="form-control input-sm" placeholder="名称" />
        <button id="btnFilter" class="btn btn-default btn-sm" title="查询"><i class="glyphicon glyphicon-search"></i></button>
      </div>
      <div id="window-main-table-container"><span class="tooltip-loading-text "><img src="/resources/images/loading.gif" alt="正在加载..." /></span></div>
    </div>
    <div id="window-main-table-footer" class="table-footer x-freeze-height"></div>
  </div>
</div>