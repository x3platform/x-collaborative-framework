  <statements>
    <insert id="proc_Entity_MetaData_Insert" parameterClass="EntityMetaDataInfo">
      INSERT INTO tb_Entity_MetaData
      (
        Id,
        EntitySchemaId,
        FieldName,
        FieldType,
        Description,
        DataColumnName,
        EffectScope,
        Locking,
        OrderId,
        Status,
        Remark,
        ModifiedDate,
        CreatedDate
      )
      VALUES
      (
        #Id#,
        #EntitySchemaId#,
        #FieldName#,
        #FieldType#,
        #Description#,
        #DataColumnName#,
        #EffectScope#,
        #Locking#,
        #OrderId#,
        #Status#,
        #Remark#,
        CURRENT_TIMESTAMP,
        CURRENT_TIMESTAMP
      )
    </insert>
    <update id="proc_Entity_MetaData_Update" parameterClass="EntityMetaDataInfo">
      UPDATE tb_Entity_MetaData SET

        FieldName = #FieldName#,
        FieldType = #FieldType#,
        Description = #Description#,
        DataColumnName = #DataColumnName#,
        EffectScope = #EffectScope#,
        Locking = #Locking#,
        OrderId = #OrderId#,
        Status = #Status#,
        Remark = #Remark#,
        ModifiedDate = CURRENT_TIMESTAMP

