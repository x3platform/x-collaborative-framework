<?xml version="1.0" encoding="utf-8" ?>
<!--
    /*
     * The entities application block configuration file.
     *
     * Update : 2010.01.01
     */
-->
<configuration>
    <configSections>
        <section name="entitiesConfiguration"  type="X3Platform.Entities.Configuration.EntitiesConfiguration,X3Platform.Entities" />
    </configSections>

    <!-- entities configuration begin -->

    <entitiesConfiguration>
        <keys>
            <!-- SQL Server : 数据库连接名称 -->
            <add name="DataSourceName" value="ConnectionString" />
            <!-- IBatis : 配置映射文件 -->
            <add name="IBatisMapping" value="config\X3Platform.Entities.IBatis.SqlMap.config" />
        </keys>
    </entitiesConfiguration>

    <!-- entities configuration end -->

</configuration>