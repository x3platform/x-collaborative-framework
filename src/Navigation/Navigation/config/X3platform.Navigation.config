<?xml version="1.0" encoding="utf-8" ?>
<!--
    /*
     * The navigation application block configuration file.
     *
     * Update : 2010.01.01
     */
     
     DataSourceName : ConnectionString (SQLServer) 
     
     IBatisMapping : config\X3Platform.Navigation.IBatis.SqlMap.config (IBatis) 
-->
<configuration>
    <configSections>
        <section name="navigationConfiguration"  type="X3Platform.Navigation.Configuration.NavigationConfiguration,X3Platform.Navigation" />
    </configSections>

    <!-- navigation configuration begin -->

    <navigationConfiguration>
        <keys>
            <!-- SQL Server : 数据库连接名称 -->
            <add name="DataSourceName" value="ConnectionString" />
            <!-- IBatis : 配置映射文件 -->
            <add name="IBatisMapping" value="config\X3Platform.Navigation.IBatis.SqlMap.config" />
        </keys>
    </navigationConfiguration>

    <!-- navigation configuration end -->

</configuration>