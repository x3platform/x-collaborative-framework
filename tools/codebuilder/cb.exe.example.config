  <codeBuilder>
    <!-- 主名称空间 -->
    <namespaceRoot>X3Platform.Plugins.News</namespaceRoot>
    <!-- 数据库提供器 -->
    <!--
        -*- MySql -*-
        <databaseProvider assembly="X3Platform.CodeBuilder" className="X3Platform.CodeBuilder.Data.DbSchemaProviders.MySqlSchemaProvider" type="MySql" >
            <connectionString>Data Source=localhost;Initial Catalog=X3Platform.debug;Persist Security Info=True;User ID=sa;Password=sa;</connectionString>
