		-*- Oracle -*-
        <databaseProvider assembly="X3Platform.CodeBuilder" className="X3Platform.CodeBuilder.Data.DbSchemaProviders.OracleSchemaProvider" type="Oracle" >
            <connectionString>Data Source=.;User Id=test;Password=test;</connectionString>
        </databaseProvider>
		-*- SqlServr -*-
        <databaseProvider assembly="X3Platform.CodeBuilder" className="X3Platform.CodeBuilder.Data.DbSchemaProviders.SqlServerSchemaProvider" type="SqlServer" ownerName="dbo">
            <connectionString>Data Source=(local);Initial Catalog=X3_Testing;Persist Security Info=True;User ID=sa;Password=sa;</connectionString>
        </databaseProvider>
		-->
    <databaseProvider assembly="X3Platform.CodeBuilder" className="X3Platform.CodeBuilder.Data.MySqlSchemaProvider" type="MySql" >
      <connectionString>Data Source=localhost;Initial Catalog=X3Platform.debug;Persist Security Info=True;User ID=sa;Password=sa;</connectionString>
